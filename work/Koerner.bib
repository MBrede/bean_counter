  keywords = {BSE,Ceramic,Grain size,Segmentation,SEM},
  file = {/home/brede/Zotero/storage/LLCZUQCF/Podor et al. - 2021 - SEraMic A semi-automatic method for the segmentat.pdf;/home/brede/Zotero/storage/C2CZD4NY/S0955221921002326.html}
